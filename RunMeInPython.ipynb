   "source": [
    "n = int(input('Write the value of n: '))\n",
    "l1=1 #Initial condition\n",
    "l2=3 #Initial condition\n",
    "lucas=[l1, l2]\n",
    "\n",
    "print('1: ',l1)\n",
    "print('2: ', l2)\n",
    "\n",
    "start1 = time.time()\n",
    "for i in range(n-2):\n",
    "    l_n = l2 + l1\n",
    "    l1 = l2 \n",
    "    l2 = l_n\n",
    "    lucas.append(l_n)\n",
    "    print(i+3, ':', l_n)\n",
    "end1 = time.time()\n",
    "\n",
    "print('Tiempo de ejecución: ',timedelta(seconds=end1-start1))\n",
    "plt.plot([i for i in range(n)], lucas)"
   ]
